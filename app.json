{
  "expo": {
    "name": "모두의 청첩장",
    "slug": "모두의 청첩장",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/modu.png",
    "scheme": "mocheong",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/modu.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tkman.mocheong"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/modu.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/modu.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f07229d6-641e-4e63-bcf5-8bbebd694927"
      }
    }
  }
}
